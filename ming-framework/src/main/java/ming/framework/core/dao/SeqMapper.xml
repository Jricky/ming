<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ming.framework.core.dao.SeqMapper">
    <select id="getSeq" parameterType="java.lang.String" resultType="java.lang.String" statementType="STATEMENT"
            useCache="false" flushCache="true">
		select nextval(${seqName}) as id
	</select>
    <select id="getTimeSeq" parameterType="java.lang.String" resultType="java.lang.String" statementType="STATEMENT"
            useCache="false" flushCache="true">
		select concat(date_format(now()'%Y%m%d%H%m%s'),nextval(${seqName})) as id
	</select>
</mapper>
